package ch05;
// 얕은 복사 // 깊은 복사. 
public class ArrayCopy { 

	public static void main(String[] args) {
		   int [] list= {10,20,30,40,50}; 
		  // 배열 복사 방법 2가지 
		  // 1) 얕은 복사 : 배열의 주소값을 공유해서 복사. 
		   int []numbers = list; 
		   for(int i= 0; i< numbers.length; i++) {
			   System.out.println(numbers[i]);
			   
		   }
		   
	}

}
