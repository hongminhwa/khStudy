package ch08;

public class CalculatorUse {   
	public static void main(String[] args) {
		Calculator c1 = new Calculator(); // 객체 생성 및 기본 생성자 호출 
		// 입력 
		c1.setLeft(10);
		c1.setRight(20); 
		//출력 
		System.out.println(c1.getLeft());
		System.out.println(c1.getRight()); 
		c1.sum(); 
		c1.avg();
		

	}

}
